{
  "name": "resume-builder",
  "version": "1.0.0",
  "description": "ATS Certified Resume",
  "author": "Ethan G",
  "license": "MIT",
  "private": true,
  "keywords": [
    "next",
    "nextjs",
    "react",
    "reactjs",
    "tailwind",
    "tailwindcss",
    "prettier",
    "eslint",
    "vercel",
    "resume",
    "resumebuilder"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "next": "14.2.5",
    "next-mdx-remote": "^5.0.0",
    "pdf-lib": "^1.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
